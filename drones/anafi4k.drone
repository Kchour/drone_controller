<?xml version="1.0" encoding="UTF-8"?>
<!-- Obtained from /opt/parrot-sphinx/usr/share/sphinx/drones -->
<!-- With additional parameters from online site -->
<drone
  name="anafi4k"
  firmware="http://plf.parrot.com/sphinx/firmwares/anafi/pc/latest/images/anafi-pc.ext2.zip"
  hardware="anafi4k">
  <machine_params
    low_gpu="0"
    with_front_cam="1"
    simple_front_cam="1"
    with_gimbal="1"
    with_kalamos="0"
    sdcard_serial="__undefined__"
    product_pro="0"/>
  <pose>default</pose>
  <!--  -->
  <!-- Simulate drone ethernet interface -->
  <!-- make sure to replaced with actual eth interface name -->
  <!--  -->
  <!-- <interface>eth1</interface> -->
  <interface>enp59s0</interface>

  <!--  -->
  <!-- 'wlan0' may need to be replaced the actual wifi interface name -->
